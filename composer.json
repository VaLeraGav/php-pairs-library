{
  "name": "halon/php-pairs-library",
  "description": "Pairs and Lists implementation",
  "license": "MIT",
  "keywords": [
    "pairs"
  ],
  "scripts": {
    "phpunit": "phpunit",
    "phpcs": "phpcs"
  },
  "authors": [
    {
      "name": "Kirill Mokevnin",
      "email": "mokevnin@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4 || >=8.1"
  },
  "autoload": {
    "files": [
      "src/Pairs.php",
      "src/Lists.php",
      "src/HtmlTags.php",
      "lessons/02_RepresentationOfSequences.php",
      "lessons/03_Markup.php",
      "lessons/04_expressionOfLists.php",
      "lessons/05_filtrationOfLists.php",
      "lessons/07_standardInterfaces.php",
      "lessons/08_ierarchicalStructures.php",
      "lessons/06_convert.php"
    ],
    "psr-4": {
      "App\\": "src/",
      "App\\Solution\\": "lessons/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "*",
    "phpunit/phpunit": "^8.3"
  }
}
